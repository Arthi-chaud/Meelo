{
	"root": true,
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": [
			"./tsconfig.json"
		]
	},
	"plugins": [
		"@typescript-eslint"
	],
	"ignorePatterns": ["front/", "node_modules/", "test/"],
	"rules": {
		"@typescript-eslint/switch-exhaustiveness-check": 2
	}
}