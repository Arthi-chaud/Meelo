server {
	listen ${PORT} default_server;
	listen [::]:${PORT} default_server;
	access_log off;
	server_name _;

	location = /api {
	    return 302 /api/;
	}
	location /api/ {
	    proxy_pass ${SERVER_URL}/;
	}

	location /docs {
		alias /app/docs;
		index index.html; 
	}
	location / {
		proxy_pass ${FRONT_URL};
	}
}